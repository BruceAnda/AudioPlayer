<?xml version="1.0" encoding="UTF-8"?>
<!-- 白色的矩形框，用于gridview背景 -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >

    <solid android:color="#b0000000" />

    <stroke
        android:width="2dip"
        android:color="#b4ffffff" />

    <padding
        android:bottom="7dip"
        android:left="7dip"
        android:right="7dip"
        android:top="7dip" />

    <corners android:radius="10dip" />

</shape>